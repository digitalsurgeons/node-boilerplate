{
  "name": "ds-node-boilerplate",
  "version": "1.0.0",
  "description": "Starting point for DS Node.js projects.",
  "main": "index.js",
  "browser": {
    "level": "level-browserify"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "start": "node index.js",
    "watch": "watchify browser/js/index.js -o browser/dist/bundle.js -dv",
    "build": "browserify browser/js/index.js -g uglifyify | uglifyjs -cm > browser/dist/bundle.js",
    "dev": "npm run watch & npm start",
    "production": "npm run build & npm start",
    "lint": "eslint **/*.js",
    "test": "npm run lint && tape test/*.js",
    "baitAndSwitch": "standard-format -w **/*.js"
  },
  "keywords": [
    "DS",
    "Node",
    "Node.js",
    "Boilerplate",
    "starter"
  ],
  "author": "Digital Surgeons",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "oppressor": "0.0.1",
    "standard-format": "^2.2.1",
    "tape": "^4.6.0",
    "uglify-js": "^2.6.3",
    "uglifyify": "^3.0.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "JSONStream": "^1.1.2",
    "body": "^5.1.0",
    "bytewise": "^1.1.0",
    "cookie": "^0.3.1",
    "emit-stream": "^0.1.2",
    "end-of-stream": "^1.1.0",
    "has": "^1.0.1",
    "hyperstream": "^1.2.2",
    "level": "^1.4.0",
    "level-browserify": "^1.1.0",
    "mux-demux": "^3.7.9",
    "patterns": "^1.0.2",
    "pump": "^1.0.1",
    "split2": "^2.1.0",
    "st": "^1.1.0",
    "through2": "^2.0.1",
    "websocket-stream": "^3.2.1",
    "xhr": "^2.2.0"
  }
}
